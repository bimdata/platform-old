{
  "name": "bimdata-platform",
  "version": "1.0.0",
  "description": "BIMData.io Platform",
  "main": "index.js",
  "scripts": {
    "serve": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config build/webpack.config.dev.js --port 8080",
    "build": "node --max-old-space-size=4000 ./node_modules/.bin/webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "unit": "jest --config test/unit/jest.config.js",
    "unit:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js",
    "test": "npm run unit",
    "generate-icons": "vsvg -s ./src/assets/images/icons/compiled -t ./src/icons --es6",
    "optimize-svg": "svgo -f ./src/assets/images/icons -o ./src/assets/images/icons/compiled",
    "import-material-icon": "node build/get-material-icons.js",
    "svg-process": "npm run import-material-icon && npm run optimize-svg && npm run generate-icons"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "Bimdata",
  "license": "MIT",
  "dependencies": {
    "@bimdata/background-color-viewer-plugin": "1.0.0",
    "@bimdata/bimdata-api-client": "~6.2.0",
    "@bimdata/bimobject-viewer-plugin": "^1.0.0",
    "@bimdata/gltf-extractor-viewer-plugin": "^1.0.0",
    "@bimdata/idex-viewer-plugin": "1.0.0",
    "@bimdata/iot-viewer-plugin": "^1.0.7",
    "@bimdata/realiz3d-viewer-plugin": "0.0.2",
    "@bimdata/svg-extractor-viewer-plugin": "1.0.0",
    "@bimdata/utils": "0.2.3",
    "@bimdata/viewer": "^1.4.1-rc.4",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.0.0-rc.28",
    "isemail": "^3.2.0",
    "mapbox-gl": "^0.53.1",
    "moment": "^2.24.0",
    "oidc-client": "^1.9.1",
    "scriptjs": "^2.5.9",
    "seedrandom": "^3.0.5",
    "uppy": "0.26.0",
    "velocity-animate": "^1.5.2",
    "vue": "2.5.22",
    "vue-clickaway": "^2.2.2",
    "vue-i18n": "^8.14.0",
    "vue-router": "^3.1.3",
    "vue-select": "^2.6.4",
    "vue-svgicon": "^3.2.6",
    "vuex": "^3.1.1",
    "vuex-oidc": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@mdi/svg": "^3.9.97",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chromedriver": "^2.46.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "imports-loader": "^0.8.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.3.1",
    "selenium-server": "^3.141.59",
    "style-loader": "^0.23.1",
    "svgo": "^1.3.0",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "vsvg": "^1.9.1",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-server-renderer": "2.5.22",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.5.22",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --ext .js,.vue src"
    }
  }
}
